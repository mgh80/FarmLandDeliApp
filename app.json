{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "cache": {
        "disabled": true
      },
      "env": {
        "EAS_NO_VCS": "1",
        "EXPO_SKIP_AUTOLINKING": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "cache": {
        "disabled": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
